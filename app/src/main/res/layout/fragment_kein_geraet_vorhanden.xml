<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ch.msacchi.gymplaner.KeinGeraetVorhanden">

    <!-- TODO: Update blank fragment layout -->


    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/tvButtonListeWechseln"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="0dp"
        android:background="@color/myButtonBackground"
        android:padding="5dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="51dp"
        android:gravity="center_vertical|center_horizontal"
        android:text="@string/kein_geraet_vorhanden"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="114dp"
        tools:layout_conversion_absoluteWidth="384dp"
        tools:layout_conversion_absoluteHeight="51dp"
        android:layout_marginTop="105dp" />

    <ImageButton
        android:id="@+id/btnGoToCreatGeraet"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:adjustViewBounds="true"
        android:background="#00000000"
        android:contentDescription="PlusButton"
        android:onClick="goToGeraetErstellen"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textGeraetNeu"
        app:srcCompat="@drawable/plus"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_conversion_absoluteHeight="50dp"
        tools:layout_conversion_absoluteWidth="50dp"
        tools:layout_conversion_absoluteX="167dp"
        tools:layout_conversion_absoluteY="203dp" />

    <TextView
        android:id="@+id/textGeraetNeu"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:gravity="center_vertical|center_horizontal"
        android:text="@string/lable_geraeterstellen"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:layout_conversion_absoluteX="142dp"
        tools:layout_conversion_absoluteY="173dp"
        tools:layout_conversion_absoluteWidth="100dp"
        tools:layout_conversion_absoluteHeight="22dp"
        android:layout_marginTop="8dp" />


</android.support.constraint.ConstraintLayout>
